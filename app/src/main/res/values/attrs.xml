<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScrollConstraintLayout">
        <attr name="outside_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HwClockView">
        <attr name="timeTextSize" format="dimension" />
        <attr name="timeTextColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="GradientImageView">
        <attr name="gradient_bg" format="reference"/>
        <attr name="gradient_bg_width" format="reference|dimension"/>
        <attr name="gradient_bg_height" format="reference|dimension"/>

        <attr name="gradient_src" format="reference"/>
        <attr name="gradient_src_width" format="reference|dimension"/>
        <attr name="gradient_src_height" format="reference|dimension"/>
        <attr name="gradient_src_selected" format="reference"/>
        <attr name="gradient_src_selected_width" format="reference|dimension"/>
        <attr name="gradient_src_selected_height" format="reference|dimension"/>
    </declare-styleable>

</resources>